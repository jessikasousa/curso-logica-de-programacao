Algoritmo "exemploSenhaFixa"

Var
   senha: inteiro


Inicio

      escreva("Digite a senha: ")
      leia(senha)
      
      enquanto senha <> 2022 faca
         escreva("Senha Inválida! Tente novamente: ")
         leia(senha)
      fimenquanto
      
      escreval("Acesso permitido!")


Fimalgoritmo